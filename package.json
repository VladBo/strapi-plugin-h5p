{
  "name": "strapi-plugin-h5p",
  "version": "1.2.0",
  "description": "H5P plugin for Strapi - Create and manage interactive H5P content directly in Strapi CMS",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "h5p",
    "interactive-content",
    "e-learning",
    "education",
    "quiz",
    "cms",
    "lms",
    "assessment",
    "interactive"
  ],
  "homepage": "https://github.com/VladBo/strapi-plugin-h5p#readme",
  "bugs": {
    "url": "https://github.com/VladBo/strapi-plugin-h5p/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladBo/strapi-plugin-h5p.git"
  },
  "scripts": {
    "build": "npm run build:plugin && npm run build:scripts",
    "build:plugin": "pack-up build",
    "build:scripts": "tsc -p scripts/tsconfig.json",
    "watch": "pack-up watch",
    "verify": "pack-up check",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "setup": "node dist/scripts/setup-h5p.js",
    "prepare": "npm run build",
    "postinstall": "node dist/scripts/postinstall.js 2>/dev/null || true"
  },
  "bin": {
    "strapi-plugin-h5p": "./dist/scripts/setup-h5p.js"
  },
  "strapi": {
    "name": "h5p",
    "displayName": "H5P",
    "description": "Create and manage interactive H5P content",
    "kind": "plugin"
  },
  "exports": {
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.tsx",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "admin/src",
    "server/src",
    "scripts/src",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@lumieducation/h5p-react": "^10.0.4",
    "@lumieducation/h5p-server": "^10.0.4",
    "axios": "^1.6.0",
    "fs-extra": "^11.1.1"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intl": "^6.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@strapi/pack-up": "^5.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "typescript": "^5"
  },
  "author": {
    "name": "Vlad Bo",
    "email": "vlaad.bo@gmail.com"
  },
  "maintainers": [
    {
      "name": "Vlad Bo",
      "email": "vlaad.bo@gmail.com"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
